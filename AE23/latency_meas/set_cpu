echo "Update userspace mode for cpu scaling" 
echo userspace | sudo tee /sys/devices/system/cpu/cpu*/cpufreq/scaling_governor
echo " Check Current setspeed"
sudo cat /sys/devices/system/cpu/cpu*/cpufreq/scaling_setspeed

echo "Set All CPU Freq = 2.4GHz"
echo 2200000 | sudo tee /sys/devices/system/cpu/cpu*/cpufreq/scaling_setspeed

echo "Check All CPU Freq"
sudo cat /sys/devices/system/cpu/cpu*/cpufreq/scaling_setspeed

echo "Check Current CPU Freq"
sudo cat /sys/devices/system/cpu/cpu*/cpufreq/scaling_cur_freq


echo "systemctl stop numad"
sudo systemctl stop numad

sudo systemctl disable numad
echo "disable numad"
systemctl is-enabled numad

sudo setpci -s 6a:00.1 COMMAND=0x02
